        <meta-data
            android:name="com.baidu.lbsapi.API_KEY"
            android:value="mEwi7F4mZPgLzOcGGU9hpgpW" />
        <!-- 【必须】 请将YOUR_ACCESS_ID修改为APP的AccessId，“21”开头的10位数字，中间没空格 -->
        <meta-data
            android:name="XG_V2_ACCESS_ID"
            android:value="2100201810" />
        <!-- 【必须】 请将YOUR_ACCESS_KEY修改为APP的AccessKey，“A”开头的12位字符串，中间没空格 -->
        <meta-data
            android:name="XG_V2_ACCESS_KEY"
            android:value="AVLW18B43D2S" />
        <meta-data android:name="com.tencent.rdm.uuid" android:value="59e42712-57e6-4cff-9086-a036de89cdb9" />
        <activity
            android:name=".app.AppStart"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoTitle_FullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- 可选 -->
        <!--<activity android:name="com.pgyersdk.activity.FeedbackActivity" />-->
